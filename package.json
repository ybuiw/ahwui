{
  "version": "0.0.1",
  "private": true,
  "description": "UI Project",
  "repository": {
    "type": "git",
    "url": "https://github.com/ybuiw/ahwui.git"
  },
  "scripts": {
    "prepare": "husky install",
    "watch": "lerna exec --scope ahwui -- npm run watch",
    "build": "lerna exec --scope ahwui -- tsbb build src/*.ts --use-babel",
    "------------": "----------------案例------------------------------",
    "start": "lerna exec --scope website -- npm run start",
    "build:website": "lerna exec --scope website -- npm run build",
    "version": "lerna version --force-publish=* --no-changelog --no-git-tag-version --no-push",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "remove": "lerna exec \"rm -rf node_modules build package-lock.json\"",
    "clean": "lerna clean --yes && npm run remove"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts,less,md,json}": "prettier --write"
  },
  "workspaces": [
    "website",
    "packages"
  ],
  "devDependencies": {
    "ajv": "~8.12.0",
    "husky": "^8.0.2",
    "lerna": "^6.6.1",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "tsbb": "^4.1.5"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}